<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tomatroid"
    android:versionCode="10"
    android:versionName="1.7.14" >

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/dayTheme" >
        <!-- @style/AppTheme @android:style/Theme.Holo.Light @android:style/Theme.Holo -->
        <activity
            android:name="com.example.tomatroid.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.tomatroid.ThemeList" />
        <activity android:name="com.example.tomatroid.StatisicActivity" />
        <activity android:name="com.example.tomatroid.DatabaseList" />
        <activity android:name="com.example.tomatroid.SettingsActivity" />
        <activity android:name="com.example.tomatroid.WeeklyActivity" />

        <receiver android:name="com.example.tomatroid.util.AlarmReceiver" />
    </application>

</manifest>